{
  "name": "production_project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "serve": "npx concurrently \"npm run serve:jsonServer\" \"npm run serve:dev\"",
    "serve:jsonServer": "node ./json-server/index.js",
    "serve:dev": "webpack serve --env mode=development port=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:tsCheck": "tsc --project ./tsconfig.json --noEmit --skipLibCheck",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "npx jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "storybook:dev": "storybook dev --config-dir ./config/storybook -p 6006",
    "storybook:build": "storybook build --config-dir ./config/storybook",
    "visual:report:html": "node scripts/generate-visual-json-report.js && reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn run lint:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.23.9",
